{
  "name": "skyrocket",
  "version": "0.0.0",
  "description": "Ambitious applications aren't single threaded",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "suave": "jscs addon app tests --fix",
    "test": "ember try:testall"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runspired/skyrocket"
  },
  "engines": {
    "node": ">= 4.1.1"
  },
  "ghooks": {
    "pre-commit": "npm run suave",
    "pre-push": "npm run suave"
  },
  "author": "Chris Thoburn (@runspired)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "broccoli-stew": "^1.2.0",
    "ember-cli": "2.18.0",
    "ember-cli-app-version": "^3.1.3",
    "ember-cli-changelog": "^0.3.4",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-ic-ajax": "1.0.0",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^4.3.0",
    "ember-cli-release": "0.2.9",
    "ember-cli-sri": "2.1.1",
    "ember-cli-toolbelts": "^0.3.3",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^2.0.0",
    "ember-getowner-polyfill": "2.2.0",
    "ember-resolver": "^4.5.0",
    "ember-suave": "4.0.1",
    "ember-try": "0.2.23",
    "jscs": "^3.0.7"
  },
  "keywords": [
    "ember-addon",
    "multi threaded",
    "threading",
    "performance",
    "60fps",
    "worker",
    "serviceWorker",
    "webWorker",
    "background",
    "task",
    "service"
  ],
  "dependencies": {
    "broccoli-funnel": "^2.0.1",
    "broccoli-babel-transpiler": "^6.1.2",
    "broccoli-concat": "^3.2.2",
    "broccoli-merge-trees": "2.0.0",
    "broccoli-plugin": "^1.2.1",
    "ember-cli-babel": "^6.11.0",
    "rollup": "^0.54.1",
    "rollup-plugin-includepaths": "^0.2.2",
    "rsvp": "^4.8.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
