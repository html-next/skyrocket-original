{
  "name": "skyrocket",
  "version": "0.0.0",
  "description": "Ambitious applications aren't single threaded",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "suave": "jscs addon app tests --fix",
    "test": "ember try:testall"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runspired/skyrocket"
  },
  "engines": {
    "node": ">= 4.1.1"
  },
  "ghooks": {
    "pre-commit": "npm run suave",
    "pre-push": "npm run suave"
  },
  "author": "Chris Thoburn (@runspired)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "broccoli-stew": "^1.2.0",
    "ember-cli": "2.3.0-beta.1",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-changelog": "^0.3.4",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.2",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-ic-ajax": "0.2.4",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^1.2.1",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "2.0.0",
    "ember-cli-toolbelts": "^0.3.3",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-getowner-polyfill": "1.0.0",
    "ember-resolver": "^2.0.3",
    "ember-suave": "1.2.3",
    "ember-try": "0.0.8",
    "jscs": "^2.7.0"
  },
  "keywords": [
    "ember-addon",
    "multi threaded",
    "threading",
    "performance",
    "60fps",
    "worker",
    "serviceWorker",
    "webWorker",
    "background",
    "task",
    "service"
  ],
  "dependencies": {
    "broccoli-babel-transpiler": "^5.5.0",
    "broccoli-concat": "^2.1.0",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "1.1.1",
    "broccoli-plugin": "^1.2.1",
    "chalk": "^1.1.3",
    "debug": "^2.2.0",
    "ember-cli-babel": "^5.1.5",
    "rollup": "^0.25.3",
    "rollup-plugin-includepaths": "^0.1.0",
    "rsvp": "^3.2.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
